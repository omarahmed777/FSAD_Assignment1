<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane fx:controller="fsd.assignment.assignment1.EditStudentController" fx:id="editStudentsPane"
            xmlns:fx="http://javafx.com/fxml">
    <content>
        <GridPane vgap="20" hgap="20">
            <!--Year of Study-->
            <Label text="Year of Study" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <Label fx:id="yearStudyDisplay" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <!--Module 1-->
            <Label text="Module Option 1" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Module Option 1" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label fx:id="mod1Edit" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <ChoiceBox fx:id="mod1ChoiceEdit" GridPane.rowIndex="4" GridPane.columnIndex="1"/>
            <!--Module 2-->
            <Label text="Module Option 2" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
            <Label text="Module Option 2" GridPane.rowIndex="2" GridPane.columnIndex="2"/>
            <Label fx:id="mod2Edit" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
            <ChoiceBox fx:id="mod2ChoiceEdit" GridPane.rowIndex="4" GridPane.columnIndex="2"/>
            <!--Module 3-->
            <Label text="Module Option 3" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
            <Label text="Module Option 3" GridPane.rowIndex="2" GridPane.columnIndex="3"/>
            <Label fx:id="mod3Edit" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
            <ChoiceBox fx:id="mod3ChoiceEdit" GridPane.rowIndex="4" GridPane.columnIndex="3"/>
            <!--make use of the EditStudentController to complete the rest of the fxml-->
        </GridPane>
    </content>
</DialogPane>

